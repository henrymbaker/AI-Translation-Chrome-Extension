{
  "manifest_version": 3,
  "name": "Translator Extension",
  "version": "1.0",
  "description": "This extension translates shit",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "popup.js"],
      "matches": [ "<all_urls>" ]
    }
  ],
  "icons": {
    "16": "icons/org_trans16.png",
    "48": "icons/org_trans48.png",
    "128": "icons/org_trans128.png"
  }
}
